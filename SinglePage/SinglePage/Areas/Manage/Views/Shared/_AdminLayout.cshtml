<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>Admin</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta property="og:title" content="" />
    <meta property="og:type" content="" />
    <meta property="og:url" content="" />
    <meta property="og:image" content="" />
    <!-- Template CSS -->
    <link href="~/admin/assets/css/main.css?v=1.1" rel="stylesheet" type="text/css" />
    @RenderSection("styles", false)
</head>
<body>
    <div class="screen-overlay"></div>
    <aside class="navbar-aside" id="offcanvas_aside">
        <nav>
            <ul class="menu-aside">
                <li class="menu-item active">
                    <a class="menu-link" asp-controller="Dashboard" asp-action="Index">
                        <i class="icon material-icons md-home"></i>
                        <span class="text">Dashboard</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" asp-controller="About" asp-action="Index">
                        <i class="icon material-icons md-home"></i>
                        <span class="text">About</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" asp-controller="Experience" asp-action="Index">
                        <i class="icon material-icons md-home"></i>
                        <span class="text">Experience</span>
                    </a>
                </li>
                <li class="menu-item">
                    <a class="menu-link" asp-controller="Education" asp-action="Index">
                        <i class="icon material-icons md-home"></i>
                        <span class="text">Education</span>
                    </a>
                </li>
            </ul>
        </nav>
    </aside>
    <main class="main-wrap">
        @RenderBody()
    </main>
    <script src="~/admin/assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="~/admin/assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="~/admin/assets/js/vendors/select2.min.js"></script>
    <script src="~/admin/assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="~/admin/assets/js/vendors/jquery.fullscreen.min.js"></script>
    <script src="~/admin/assets/js/vendors/chart.js"></script>
    <!-- Main Script -->
    @RenderSection("scripts", false)
    <script src="~/admin/assets/js/main.js?v=1.1" type="text/javascript"></script>
    <script src="~/admin/assets/js/custom-chart.js" type="text/javascript"></script>
</body>
</html>
